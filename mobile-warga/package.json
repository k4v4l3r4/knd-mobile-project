{
  "name": "mobile-warga",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prestart": "node -e \"require('fs').mkdirSync('.expo-home', { recursive: true })\"",
    "start": "cross-env USERPROFILE=.\\.expo-home HOME=.\\.expo-home expo start",
    "android": "cross-env USERPROFILE=.\\.expo-home HOME=.\\.expo-home expo start --android",
    "ios": "cross-env USERPROFILE=.\\.expo-home HOME=.\\.expo-home expo start --ios",
    "web": "cross-env USERPROFILE=.\\.expo-home HOME=.\\.expo-home expo start --web",
    "pad-icon": "node scripts/padAdaptiveIcon.js",
    "release:prod": "eas update --branch production --channel production",
    "release:preview": "eas update --branch preview --channel preview"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "axios": "^1.13.3",
    "expo": "~54.0.33",
    "expo-constants": "^18.0.13",
    "expo-font": "~14.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "^8.0.11",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.23",
    "expo-secure-store": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "babel-preset-expo": "^54.0.10",
    "cross-env": "^7.0.3",
    "jimp": "^1.6.0",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
